<odoo>
  <data>
    <record id="sge_libreria.autor_form" model="ir.ui.view">
      <field name="name">sge_libreria.autor.form</field>
      <field name="model">sge_libreria.autor</field>
      <field name="arch" type="xml">
        <form string="Autor">
          <sheet>
            <group>
              <field name="name" />
              <field name="fecha_nac"></field> 
              <field name="country_id"></field> 
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="sge_libreria.autor_tree" model="ir.ui.view">
      <field name="name">sge_libreria.autor.tree</field>
      <field name="model">sge_libreria.autor</field>
      <field name="arch" type="xml">
        <tree string="Autor">
          <field name="name" />
          <field name="fecha_nac"></field>    
          <field name="country_id"></field>  
        </tree>
      </field>
    </record>
      
  </data>
</odoo>