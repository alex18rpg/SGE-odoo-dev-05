<odoo>
  <data>
    <record id="agc_gimansio.entrenador_form" model="ir.ui.view">
      <field name="name">agc_gimansio.entrenador_form</field>
      <field name="model">agc_gimansio.entrenador</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <field name="name" />
              <field name="numero_objetos"></field>
              <field name="legendario" />
            <!-- Esta vista es para las relaciones (viene de) 
              <field name="libro_ids" >
                <tree string="Libro">
                    <field name="name" />
                    <field name="precio" /> 
                </tree>
              </field>              
            -->
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="agc_gimnasio.entrenador_tree" model="ir.ui.view">
      <field name="name">agc_gimnasio.entrenador.tree</field>
      <field name="model">agc_gimnasio.entrenador</field>
      <field name="arch" type="xml">
        <tree string="Entrenador">
          <field name="name" />
          <field name="numero_objetos" />
          <field name="legendario" />     
        </tree>
      </field>
    </record>  
  </data>
</odoo>