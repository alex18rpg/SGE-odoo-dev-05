<odoo>
  <data>
    <record id="agc_gimnasio.lider_form" model="ir.ui.view">
      <field name="name">agc_gimnasio.lider_form</field>
      <field name="model">agc_gimnasio.lider</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <field name="name" />
              <field name="nivel" />
              <field name="numero_objetos" />
              <field name="legendario" />
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="agc_gimnasio.subdito_tree" model="ir.ui.view">
      <field name="name">agc_gimnasio.subdito.tree</field>
      <field name="model">agc_gimnasio.subdito</field>
      <field name="arch" type="xml">
        <tree string="Subdito">
            <field name="name" />
            <field name="nivel" />
            <field name="numero_objetos" />
            <field name="legendario" />   
        </tree>
      </field>
    </record>    
  </data>
</odoo>