<odoo>
  <data>
    <record id="agc_gimnasio.equipo_form" model="ir.ui.view">
      <field name="name">agc_gimnasio.equipo_form</field>
      <field name="model">agc_gimnasio.equipo</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <field name="name" />
              <field name="apodo"></field>
              <field name="numero_miembros" />
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="agc_gimnasio.equipo_tree" model="ir.ui.view">
      <field name="name">agc_gimnasio.equipo.tree</field>
      <field name="model">agc_gimnasio.equipo</field>
      <field name="arch" type="xml">
        <tree string="Equipo">
          <field name="name" />
          <field name="apodo" />
          <field name="numero_miembros" />     
        </tree>
      </field>
    </record>
      
  </data>
</odoo>